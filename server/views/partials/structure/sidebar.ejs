<nav id="sidebar-menu" class="col-12 col-lg-3 col-md-3 col-xl sidebar-nav position-relative">
  <div class="pt-md-4 pb-md-5">
    <ul class="nav d-flex flex-md-column g-4 sidebar pb-3 pt-md-3 pb-md-5 align-items-center">
        <%
        menu.forEach(function(item) {
          const active = item.key === page ? 'active' : '';
          const currentPage = active ? 'aria-current="page"' : '';
        %>
          <li class="nav-item fs-5 fs-xl-4">
            <a class="nav-link d-flex justify-content-between rounded <%= active %>" <%= currentPage %> href="<%= item.url %>">
              <%= item.title %>
            </a>
          </li>
          <% }); %>
    </ul>
  </div>
</nav>
